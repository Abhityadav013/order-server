{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/v1/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/webhook/(.*)",
      "dest": "index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "index.ts"
    }
  ]
}
